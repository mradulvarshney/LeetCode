class Solution 
{
    public int smallestEvenMultiple(int n) 
    {
        int t = 0;
        int i = 1;
        while(true)
        {
            if(i%2==0 && i%n==0)
            {
                t = 1;
                break;
            }
            i++;
        }
        
        return i;
    }
}
